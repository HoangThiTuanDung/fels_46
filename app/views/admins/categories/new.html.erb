<% provide(:title, "New Category") %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>New Category</h1>
    <%= form_for [:admins, @category] do |f| %>
      <%= render "shared/error_messages", object: f.object %>
      <%= render 'category_fields',f: f %>
      <%= f.submit "Create", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>